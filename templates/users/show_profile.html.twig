{% extends 'base.html.twig' %}

{% block title %}Profile{% endblock %}


{% block body %}
    <div class="card">
        <div class="card-header">
            <h3>Profile</h3>
        </div>

        <div class="card-body">
            <h4>{{ user.username }}</h4>
            <p>{{ user.email }}</p>
            <p>{{ user.fullname }}</p>
        </div>
    </div>
{% endblock %}
